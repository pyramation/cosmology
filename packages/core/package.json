{
  "name": "@webql/core",
  "version": "0.20.0",
  "description": "core UI components",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/pyramation/web/tree/master/packages/core#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "main/index.js",
  "module": "module/index.js",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "sideEffects": false,
  "files": [
    "main",
    "module"
  ],
  "exports": {
    ".": {
      "require": "./main/index.js",
      "default": "./module/index.js"
    }
  },
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyramation/web"
  },
  "scripts": {
    "make:icons": "node ./scripts/make-icons",
    "build:main": "cross-env BABEL_ENV=production babel src --out-dir main --ignore 'src/**/*.stories.js','src/**/*.test.js'",
    "build:module": "cross-env MODULE=true babel src --out-dir module --ignore 'src/**/*.stories.js','src/**/*.test.js'",
    "build:module:watch": "cross-env MODULE=true babel src --out-dir module --watch --ignore 'src/**/*.stories.js','src/**/*.test.js'",
    "build": "npm run build:module && npm run build:main",
    "prebuild": "rimraf main module",
    "prepare": "npm run build",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint src --ext .js",
    "lint:fix": "eslint src --fix",
    "lint:style": "stylelint 'src/**/*.js'",
    "clean": "rm -rf node_modules",
    "storybook": "start-storybook -p 9999",
    "storybook:build": "build-storybook"
  },
  "bugs": {
    "url": "https://github.com/pyramation/web/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.12.13",
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.13",
    "@babel/plugin-transform-runtime": "7.12.13",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "7.12.13",
    "@storybook/addon-actions": "6.1.9",
    "@storybook/addon-knobs": "6.1.9",
    "@storybook/addon-links": "6.1.9",
    "@storybook/addon-storyshots": "6.1.9",
    "@storybook/addon-storysource": "6.1.9",
    "@storybook/addon-viewport": "6.1.9",
    "@storybook/addons": "6.1.9",
    "@storybook/react": "6.1.9",
    "@testing-library/dom": "^7.26.6",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^27.0.2",
    "babel-plugin-inline-react-svg": "1.1.1",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-styled-components": "1.10.7",
    "cross-env": "7.0.3",
    "date-fns": "^2.16.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "4.0.0",
    "faker": "^4.1.0",
    "jest": "^27.0.4",
    "jest-axe": "^5.0.1",
    "mutation-observer": "^1.0.3",
    "prettier": "^1.19.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "3.0.2",
    "stylelint": "13.3.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@popperjs/core": "^2.4.4",
    "@reach/auto-id": "^0.10.0",
    "@react-aria/button": "^3.3.0",
    "@react-aria/focus": "^3.2.2",
    "@react-aria/interactions": "^3.2.1",
    "@react-aria/menu": "^3.2.0",
    "@react-aria/textfield": "^3.2.2",
    "@react-aria/utils": "^3.3.0",
    "@react-stately/menu": "^3.2.1",
    "@react-stately/tree": "^3.1.3",
    "@styled-system/css": "^5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2",
    "@webql/hooks": "^0.16.1",
    "color": "^3.1.2",
    "downshift": "^6.1.0",
    "fast-deep-equal": "3.1.3",
    "match-sorter": "^4.2.0",
    "mousetrap": "^1.6.5",
    "polished": "4.1.3",
    "react-datepicker": "^3.3.0",
    "react-fast-compare": "^3.0.1",
    "react-highlight-words": "^0.17.0",
    "react-popper": "^2.2.3",
    "react-spring": "^8.0.27",
    "react-virtual": "^2.5.0",
    "styled-normalize": "^8.0.7",
    "styled-system": "^5.1.5"
  },
  "peerDependencies": {
    "date-fns": "^2.16.1",
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": "^5.2.1"
  },
  "gitHead": "ba3f3376b6a839f4d81fdac4eeb2e66343bc6f6b"
}