{
  "name": "@webql/hooks",
  "version": "0.16.1",
  "description": "react hooks",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/pyramation/web/tree/master/packages/hooks#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "main/index.js",
  "module": "module/index.js",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "main",
    "module"
  ],
  "exports": {
    ".": {
      "require": "./main/index.js",
      "default": "./module/index.js"
    }
  },
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyramation/web"
  },
  "scripts": {
    "build:main": "cross-env BABEL_ENV=production babel src --out-dir main --ignore 'src/**/*.test.js'",
    "build:module": "cross-env MODULE=true babel src --out-dir module --ignore 'src/**/*.test.js'",
    "build:module:watch": "cross-env MODULE=true babel src --out-dir module --watch --ignore 'src/**/*.test.js'",
    "build": "npm run build:module && npm run build:main",
    "prebuild": "rimraf main module",
    "prepare": "npm run build",
    "lint": "eslint src --fix",
    "test": "jest --env=jsdom --passWithNoTests",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "bugs": {
    "url": "https://github.com/pyramation/web/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.12.13",
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.13",
    "@babel/plugin-transform-runtime": "7.12.13",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "7.12.13",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-macros": "2.8.0",
    "cross-env": "7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "5.2.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "4.0.0",
    "jest": "^27.0.4",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.2",
    "fast-deep-equal": "^3.1.3",
    "intersection-observer": "^0.11.0",
    "react": "^17.0.2",
    "resize-observer-polyfill": "1.5.1"
  },
  "gitHead": "ba3f3376b6a839f4d81fdac4eeb2e66343bc6f6b"
}